    "version": "0.2.0",
    "configurations": [
		{
			"name": "checker",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/checker",
			"args": ["4" ,"2","13", "53", "112","54", "45", "11"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb"
		},
		{
			"name": "(lldb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/push_swap",
			"args": ["2" ,"1","3", "4", "5"],
			"stopAtEntry": true,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb"
		},

		{
			"name": "(lldb) Attach",
			"type": "cppdbg",
			"request": "attach",
			"program": "${workspaceFolder}/push_swap",
			"args": ["2" ,"1","0"] ,
			"processId": "${command:pickProcess}",
			"MIMode": "lldb"
		},
        {
            "name": "push_swap",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/push_swap",
			"args": ["2" ,"1","0"] ,
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}